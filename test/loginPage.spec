@objects
	form 			id general-form
	username        id inputEmail
	password 		id inputPassword
	loginButton		css #general-form > div.user-form-button-wrap > button
	cbtIcon			css body > div.wrapper > div.app-container > div.app.ng-scope > div > div > div.cbt-user-form-header > a > img
	sbIcon 			css #user-form-footer > img

= Main section =
	username:
		width ~ 340px
		height ~ 40px
		inside form
	
	password:
		width ~ 340px
		height ~ 40px
		inside form

	loginButton:
		width ~ 340px
		height ~ 50px
	
	cbtIcon:
		width ~ 255px
		height ~ 38px
	
	sbIcon:
		width ~	100px
		height ~ 13px
